def custom_routes(account_id, key, proxy, current_session_modules):
    # functions_modules = [module_functions[module] for module in current_session_modules]

    functions_modules = [(module_name, module_functions[module_name]) for module_name in current_session_modules]

    random_module = False  # не надо трогать, модули и так перемешиваются в процессе формирования сессии
    # count_of_tx = random.randint(1, len(use_modules))
    count_of_tx = None

    routes_maker = Routes(account_id, key, proxy)
    routes_maker.start(functions_modules, sleep_from, sleep_to, random_module, count_of_tx)
